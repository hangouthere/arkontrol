    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-jwt": "^3.6.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "log4js": "^6.0.0",
    "node-fetch": "^2.6.0",
    "rcon-ts": "^1.2.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.10.1",
    "redux": "^4.0.4",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "sqlite": "^3.0.3",
    "ts-koa-connect-history-api-fallback": "^0.4.3",
    "type-to-reducer": "^1.2.0",
    "wretch": "^1.5.5",
    "ws": "^7.2.0"
  },
  "excludes": {
    "frontEnd": [
      "@koa/cors",
      "fs-extra",
      "log4js",
      "jsonwebtoken",
      "koa-bodyparser",
      "koa-compose",
      "koa-jwt",
      "koa-router",
      "koa-static",
      "koa",
      "node-fetch",
      "rcon-ts",
      "sqlite",
      "ws"
    ]
