{
  "private": true,
  "name": "arkontrol",
  "version": "1.0.0",
  "description": "Kontrol your Ark server with ArKontrol",
  "author": "nfgCodex",
  "license": "ISC",
  "main": "_dist/backend/index.js",
  "keywords": [],
  "scripts": {
    "postinstall": "cross-env FORCE_BUILD_DLL=1 npm run build:dev:frontend_dll",
    "start": "cd ./_dist/backend && node ./index.js",
    "start:build": "cd ./_build && index-win.exe",
    "start:dev": "concurrently -c black.bgCyan,cyan -n BackEnd,FrontEnd \"npm run start:dev:backend\" \"npm run start:dev:frontend\"",
    "start:dev:backend": "concurrently -c black.bgMagenta,magenta -n Compile,Runtime \"cross-env HOT=1 npm run build:dev:backend -- --watch\" \"sleep 10 && nodemon --inspect -w ./_dist/backend -i stats/* ./_dist/backend/index.js\"",
    "start:dev:frontend": "cross-env HOT=1 webpack-dev-server --hot --config ./.config/webpack.frontend.config.js",
    "build:dev": "concurrently -c black.bgCyan,cyan -n BackEnd,FrontEnd \"npm run build:dev:backend\" \"npm run build:dev:frontend\"",
    "build:dev:frontend_dll": "cross-env NODE_ENV='development' webpack --display-modules --config ./.config/webpack.frontend_dll.config.js",
    "build:dev:frontend": "cross-env NODE_ENV='development' webpack --display-modules --config ./.config/webpack.frontend.config.js",
    "build:dev:backend": "cross-env NODE_ENV='development' webpack --display-modules --config ./.config/webpack.backend.config.js",
    "build:prod": "concurrently -c black.bgCyan,cyan -n BackEnd,FrontEnd \"npm run build:prod:backend\" \"npm run build:prod:frontend\"",
    "build:prod:frontend": "cross-env NODE_ENV='production' webpack --display-modules --config ./.config/webpack.frontend.config.js",
    "build:prod:backend": "cross-env NODE_ENV='production' webpack --display-modules --config ./.config/webpack.backend.config.js",
    "build:release": "pkg --out-dir ./_build ./_dist/backend/index.js",
    "clean": "rimraf ./_build ./_dist ./node_modules/.cache/babel-loader",
    "lint": "tslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=7"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@blueprintjs/core": "^3.15.0",
    "@blueprintjs/table": "^3.5.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@koa/cors": "^3.0.0",
    "core-js": "^3.0.0",
    "fs-extra": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "log4js": "^4.1.0",
    "node-fetch": "^2.3.0",
    "rcon-client": "^3.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "sqlite": "^3.0.3",
    "type-to-reducer": "^1.1.0",
    "wretch": "^1.5.1",
    "ws": "^6.2.1"
  },
  "excludes": {
    "frontEnd": [
      "@koa/cors",
      "fs-extra",
      "log4js",
      "jsonwebtoken",
      "koa-bodyparser",
      "koa-compose",
      "koa-jwt",
      "koa-router",
      "koa-static",
      "koa",
      "node-fetch",
      "rcon-client",
      "sqlite",
      "ws"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/event-kit": "^2.4.0",
    "@types/fs-extra": "^5.0.5",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^2.2.3",
    "@types/node-fetch": "^2.1.7",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.1",
    "@types/source-map-support": "^0.5.0",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/webpack-env": "^1.13.9",
    "@types/ws": "^6.0.1",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.1",
    "html-webpack-include-assets-plugin": "1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "pkg": "^4.3.7",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.11",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1",
    "useref": "^1.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
