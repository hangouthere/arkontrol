{
  "private": true,
  "name": "ark-rcon-tools",
  "version": "1.0.0",
  "description": "RCon Tools for Ark Server",
  "author": "nfgCodex",
  "license": "ISC",
  "main": "_dist/backend/index.js",
  "keywords": [],
  "scripts": {
    "postinstall": "npm run build:dev:frontend_dll",
    "start": "node ./_dist/backend/index.js",
    "dev": "concurrently -c black.bgYellow,black.bgCyan,cyan -n \"BackEnd Compile\",BackEnd,FrontEnd \"cross-env HOT=1 npm run build:dev:backend -- --watch\" \"sleep 10 && nodemon --inspect --watch ./_dist/backend ./_dist/backend/index.js\" \"cross-env HOT=1 webpack-dev-server --hot --config ./.config/webpack.frontend.config.js",
    "build:dev": "concurrently -c black.bgCyan,cyan -n BackEnd,FrontEnd \"npm run build:dev:backend\" \"npm run build:dev:frontend\"",
    "build:dev:frontend_dll": "cross-env NODE_ENV='development' webpack --display-modules --config ./.config/webpack.frontend_dll.config.js",
    "build:dev:frontend": "cross-env NODE_ENV='development' webpack --display-modules --config ./.config/webpack.frontend.config.js",
    "build:dev:backend": "cross-env NODE_ENV='development' webpack --display-modules --config ./.config/webpack.backend.config.js",
    "build:prod": "concurrently -c black.bgCyan,cyan -n BackEnd,FrontEnd \"npm run build:prod:backend\" \"npm run build:prod:frontend\"",
    "build:prod:frontend": "cross-env NODE_ENV='production' webpack --display-modules --config ./.config/webpack.frontend.config.js",
    "build:prod:backend": "cross-env NODE_ENV='production' webpack --display-modules --config ./.config/webpack.backend.config.js",
    "clean": "rimraf ./_dist ./node_modules/.cache/babel-loader",
    "lint": "tslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=7"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "core-js": "^3.0.0",
    "fs-extra": "^7.0.1",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^5.0.1",
    "lodash": "^4.17.11",
    "rcon-client": "^3.1.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  },
  "excludes": {
    "frontEnd": [
      "fs-extra",
      "koa-static",
      "koa-websocket",
      "koa",
      "rcon-client"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/event-kit": "^2.4.0",
    "@types/fs-extra": "^5.0.5",
    "@types/koa": "^2.0.48",
    "@types/koa-static": "^4.0.1",
    "@types/koa-websocket": "^5.0.3",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-server": "^3.1.4",
    "@types/webpack-env": "^1.13.9",
    "@types/ws": "^6.0.1",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "html-webpack-include-assets-plugin": "1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "useref": "^1.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
